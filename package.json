{
  "name": "s3pull",
  "version": "1.0.0",
  "description": "Thin S3 download CLI with ETag-based local caching and YAML manifest support",
  "type": "module",
  "bin": {
    "s3pull": "./bin/s3pull.mjs",
    "s3pull-all": "./bin/s3pull-all.mjs"
  },
  "files": [
    "bin/",
    "lib/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "build": "node scripts/build.mjs",
    "prepublishOnly": "yarn lint && yarn test"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "s3",
    "download",
    "cache",
    "etag",
    "fixtures",
    "cli",
    "hetzner",
    "minio",
    "object-storage",
    "s3-compatible"
  ],
  "author": "Alexander Zeitler",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/azeitler/s3pull.git"
  },
  "homepage": "https://github.com/azeitler/s3pull#readme",
  "bugs": {
    "url": "https://github.com/azeitler/s3pull/issues"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "aws-sdk-client-mock": "^4.1.0",
    "esbuild": "^0.27.3",
    "postject": "^1.0.0-alpha.6",
    "vitest": "^4.0.18"
  }
}
